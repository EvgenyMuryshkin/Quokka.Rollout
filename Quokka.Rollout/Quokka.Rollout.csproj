<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.3.40</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Evgeny Muryshkin</Authors>
    <Company>Quokka Robotics</Company>
    <Description>Tools for building and publishing nuget packages</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/EvgenyMuryshkin/Quokka.Rollout</PackageProjectUrl>
    <RepositoryUrl>https://github.com/EvgenyMuryshkin/Quokka.Rollout</RepositoryUrl>
    <RepositoryType>Public</RepositoryType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SharpZipLib" Version="1.3.1" />
  </ItemGroup>
</Project>